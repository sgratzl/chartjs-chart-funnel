{
  "name": "chartjs-chart-funnel",
  "description": "Chart.js module for charting funnel charts",
  "version": "4.0.0",
  "author": {
    "name": "Samuel Gratzl",
    "email": "samu@sgratzl.com",
    "url": "https://www.sgratzl.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/sgratzl/chartjs-chart-funnel",
  "bugs": {
    "url": "https://github.com/sgratzl/chartjs-chart-funnel/issues"
  },
  "keywords": [
    "chart.js",
    "funnel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/chartjs-chart-funnel.git"
  },
  "global": "ChartFunnel",
  "type": "module",
  "main": "build/index.js",
  "module": "build/index.js",
  "require": "build/index.cjs",
  "umd": "build/index.umd.js",
  "unpkg": "build/index.umd.min.js",
  "jsdelivr": "build/index.umd.min.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs",
      "scripts": "./build/index.umd.min.js",
      "types": "./build/index.d.ts"
    },
    "./.pnp.cjs": "./.pnp.cjs",
    "./c": "./c"
  },
  "sideEffects": false,
  "files": [
    "build",
    "src/**/*.ts",
    "c"
  ],
  "peerDependencies": {
    "chart.js": ">=3.7.0"
  },
  "browserslist": [
    "Firefox ESR",
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "devDependencies": {
    "@chiogen/rollup-plugin-terser": "^7.1.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.4.0",
    "@types/jest-image-snapshot": "^6.1.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@yarnpkg/sdks": "^2.7.0",
    "canvas": "^2.11.0",
    "canvas-5-polyfill": "^0.1.5",
    "chart.js": "^4.2.1",
    "eslint": "^8.35.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "jest-image-snapshot": "^6.1.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.3.0",
    "rollup": "^3.18.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-dts": "^5.2.0",
    "ts-jest": "^29.0.5",
    "tslib": "^2.5.0",
    "typedoc": "^0.23.26",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "clean": "rimraf --glob build docs node_modules \"*.tgz\" \"*.tsbuildinfo\"",
    "compile": "tsc -b tsconfig.c.json",
    "compile:types": "tsc -p tsconfig.c.json --emitDeclarationOnly",
    "start": "yarn run watch",
    "watch": "rollup -c -w",
    "build": "rollup -c",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --passWithNoTests --detectOpenHandles --forceExit",
    "test:watch": "yarn run test --watch",
    "test:coverage": "yarn run test --coverage",
    "samples": "yarn tsc samples/type_test.ts",
    "lint": "yarn run eslint && yarn run prettier",
    "fix": "yarn run eslint:fix && yarn run prettier:write",
    "prettier:write": "prettier \"*\" \"*/**\" --write",
    "prettier": "prettier \"*\" \"*/**\" --check",
    "eslint": "eslint src --ext .ts,.tsx --cache",
    "eslint:fix": "yarn run eslint --fix",
    "docs": "typedoc src/index.ts",
    "prepare": "yarn run build"
  },
  "packageManager": "yarn@3.4.1",
  "dependenciesMeta": {
    "@types/chroma-js@2.1.4": {
      "unplugged": true
    },
    "chart.js@4.1.1": {
      "unplugged": true
    },
    "chroma-js@2.4.2": {
      "unplugged": true
    }
  },
  "dependencies": {
    "@types/chroma-js": "^2.4.0",
    "chroma-js": "^2.4.2"
  }
}
